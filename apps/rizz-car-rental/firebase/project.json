{
  "name": "rizz-car-rental-firebase",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "tags": ["firebase:app", "firebase:name:rizz-car-rental-firebase"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "cache": true,
      "options": {
        "commands": [
          "nx run-many --target=build --projects=tag:firebase:dep:rizz-car-rental-firebase",
          "echo Build succeeded for all functions."
        ],
        "parallel": true
      }
    },    "watch": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx run-many --targets=build --projects=tag:firebase:dep:rizz-car-rental-firebase --parallel=100 --watch"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx run-many --targets=lint --projects=tag:firebase:dep:rizz-car-rental-firebase --parallel=100"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx run-many --targets=test --projects=tag:firebase:dep:rizz-car-rental-firebase --parallel=100"
      }
    },
    "firebase": {
      "executor": "nx:run-commands",
      "options": {
        "command": "firebase --config=firebase.rizz-car-rental-firebase.json --project=rizz-car-rental"
      },
      "configurations": {
        "production": {
          "command": "firebase --config=firebase.rizz-car-rental-firebase.json --project=rizz-car-rental"
        }
      }
    },
    "killports": {
      "executor": "nx:run-commands",
      "options": {
        "command": "kill-port --port 9099,5001,8080,9000,5000,8085,9199,9299,4000,4400,4500"
      }
    },
    "getconfig": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx run rizz-car-rental-firebase:firebase functions:config:get > apps/rizz-car-rental/firebase/environment/.runtimeconfig.json"
      }
    },
    "emulate": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx run rizz-car-rental-firebase:killports",
          "nx run rizz-car-rental-firebase:firebase emulators:start --import=apps/rizz-car-rental/firebase/.emulators --export-on-exit"
        ],
        "parallel": false
      }
    },
    "serve": {
      "executor": "@simondotm/nx-firebase:serve",
      "options": {
        "commands": ["nx run rizz-car-rental-firebase:watch", "nx run rizz-car-rental-firebase:emulate"]
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "nx run rizz-car-rental-firebase:firebase deploy"
      }
    }
  },
  "implicitDependencies": ["rizz-car-rental-functions-user"]
}
